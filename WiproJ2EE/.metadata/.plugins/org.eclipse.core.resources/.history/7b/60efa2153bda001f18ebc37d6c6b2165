package com.java.constructorAndObject;

public class ObjectPractice {
	
	public static void main(String[] args) {
		
		SampleObject obj = new SampleObject();
		
		obj.functionCall();
		
		System.out.println("Object variable value : "+obj.num);
		
		obj.num = 200;
		
		System.out.println("Update Object variable value : "+obj.num);
		
		System.out.println();
		
		//Static variable
		
		System.out.println("Static variable value : "+StaticKeyword.st);
		

		
		StaticKeyword.st = 500;
		
		System.out.println("Updated Static variable value : "+StaticKeyword.st);
		
		//Static function
		
		int res = StaticKeyword.add(2,3);
		
		System.out.println("Result of static function add() : "+res);
		
		System.out.println();

		//Constructor
		
		new ConstructorPractice();
		System.out.println();
		
		new ConstructorPractice(1,2);
		System.out.println();
		
		new ConstructorPractice(1000,2000);
		System.out.println();
		
		new ConstructorPractice(900,800,700);
		
	}
}
