	public static void main(String[] args) throws ClassNotFoundException {
		
		Class.forName("com.sql.cj/jdbc.Driver");
		Connection connection = DriverManager.getConnection("jdbc:mysql://localhost:3306/Transactions?useSSL=false", null, null)
	}
}
